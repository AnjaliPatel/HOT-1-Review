    <validationRules>
        <fullName>Minimum_Donation_Year_Rule</fullName>
        <active>false</active>
        <description>This rule predefines the minimum number of years required to be entered in the Donation year field.</description>
        <errorConditionFormula>Donation_Period_Years__c &lt;=  0</errorConditionFormula>
        <errorDisplayField>Donation_Period_Years__c</errorDisplayField>
        <errorMessage>Enter valid number of years i.e greater than zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Part_Payment_Amount_Rule</fullName>
        <active>true</active>
        <description>This rule validates the amount entered in the part payment field. It should not exceed the balance amount.</description>
        <errorConditionFormula>(Part_Payment__c   &gt;  Balance_Amount__c)  ||   
( Part_Payment__c   &lt; 0 )</errorConditionFormula>
        <errorDisplayField>Part_Payment__c</errorDisplayField>
        <errorMessage>Enter valid part payment. It should be greater than zero and not more than balance amount.</errorMessage>
    </validationRules>
