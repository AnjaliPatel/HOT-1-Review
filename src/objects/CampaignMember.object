        <errorConditionFormula>NOT (ISPICKVAL( Area__c ,&apos;North&apos;) ||  ISPICKVAL( Area__c ,&apos;South&apos;) ||  ISPICKVAL( Area__c ,&apos;East&apos;) ||  ISPICKVAL( Area__c,&apos;West&apos;))&amp;&amp;($Setup.ValidationSetting__c.Activate_validation_rules__c)</errorConditionFormula>
        <errorDisplayField>Area__c</errorDisplayField>
